  <meta property="og:url" content="" />

  <script>
      (function($) {
          $.get = function(key)   {
              key = key.replace(/[\[]/, '\\[');
              key = key.replace(/[\]]/, '\\]');
              var pattern = "[\\?&]" + key + "=([^&#]*)";
              var regex = new RegExp(pattern);
              var url = unescape(window.location.href);
              var results = regex.exec(url);
              if (results === null) {
                  return null;
              } else {
                  return results[1];
              }
          }
      })(jQuery);
  </script>
  <script>

      $("meta[property=og:url]").attr("content", $.get("redirect_to"));
  </script>

