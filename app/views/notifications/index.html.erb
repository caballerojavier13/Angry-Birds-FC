<h1>Listing notifications</h1>

<table>
  <tr>
    <th>Usuario</th>
    <th>Integer</th>
    <th>Noticia</th>
    <th>Integer</th>
    <th>Read</th>
    <th>Boolean</th>
    <th>Mensaje</th>
    <th>String</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @notifications.each do |notification| %>
  <tr>
    <td><%= notification.usuario_id %></td>
    <td><%= notification.integer %></td>
    <td><%= notification.noticia_id %></td>
    <td><%= notification.integer %></td>
    <td><%= notification.read %></td>
    <td><%= notification.boolean %></td>
    <td><%= notification.mensaje %></td>
    <td><%= notification.string %></td>
    <td><%= link_to 'Show', notification %></td>
    <td><%= link_to 'Edit', edit_notification_path(notification) %></td>
    <td><%= link_to 'Destroy', notification, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Notification', new_notification_path %>
